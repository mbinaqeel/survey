    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Personality Prediction | Survey</title>

    <style>
        .stork {
            -webkit-text-stroke: 1px black;
            color: white;
            text-shadow: 3px 3px 0 #000, -1px -1px 0 #000, 1px -1px 0 #000, -1px 1px 0 #000, 1px 1px 0 #000;
        }
        .fa-circle-o
        {
            margin:8px;
        }
         .qcontainer {
            margin-left: 3%;
            color: black;
        }
        .ques-grid {
            font-size: 30px;
            margin-top:80px;
        }
        .ans-grid {
            width: 100%;
            margin-left:2%;
            font-weight:bold;
        }
        img{
            width:200px;
            height:200px;
            border-radius:30%;
        }
        .img-grid {
            float:right; margin-right:50px;
        }
        .heading {
            margin-left: 3%;
            font-size: 40px;
            color: black;
            font-family:'Cooper Lt BT'
        }
        .main-container {
            margin-left: 3%;
            font-size: 30px;
            color: black;

        }
        .styled select {
            background-color: white;
            background:transparent;
            width: 150px;
            font-size: 16px;
            border: 1px solid #ccc;
            height: 34px;
        }

        .styled {
            width: 120px;
            height: 35px;
            border: 1px solid #111;
            border-radius: 3px;
            overflow: hidden;
            background: url(/Images/down-arrow.png) 96% / 20% no-repeat #ffffff;
        }

        .block {
            position: relative;
            display: inline-block;
            padding: 1em 5em 1em 1em;
            overflow: hidden;
            color: #fff;
            font-size:20px;
        }

            .block:after {
                content: '';
                position: absolute;
                top: 0;
                left: 0;
                width: 100%;
                height: 100%;
                background: #7800ff;
                -webkit-transform-origin: 100% 0;
                -ms-transform-origin: 100% 0;
                transform-origin: 100% 0;
                -webkit-transform: skew(-45deg);
                -ms-transform: skew(-45deg);
                transform: skew(-45deg);
                z-index: -1;
            }
        

    </style>
</head>
